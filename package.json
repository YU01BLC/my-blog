{
  "name": "my-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch --updateSnapshot --coverage",
    "testAuto": "jest",
    "lint": "eslint --ext .ts,.tsx ./src/app",
    "fix": "npm run import:fix && npm run format && npm run lint:fix",
    "lint:fix": "eslint --fix 'src/app/**/*.{ts,tsx}'",
